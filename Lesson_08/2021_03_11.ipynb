    "def adder(*nums):\n",
    "    slov = {}\n",
    "    for i in nums:\n",
    "        slov = {**slov, **i}\n",
    "   \n",
    "    summa = 0\n",
    "    for name, values in slov.items():\n",
    "        summa += values\n",
    "    return summa / len(slov)\n",
